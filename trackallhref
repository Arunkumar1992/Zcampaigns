	<script type="text/javascript">
$('a').click(function(){
event.preventDefault();
	selfzhref=this.href;
        selfztarget=$(this).attr('target');
        if(!selfztarget)
        {
        selfztarget="_self";
        }
zcAction('clicked', selfzhref);window.open(selfzhref,selfztarget);
}); 
</script>
